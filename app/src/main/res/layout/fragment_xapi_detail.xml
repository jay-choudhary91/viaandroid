<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/llrootlayout"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/layout_mediatype"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="18dp"
            android:background="@color/dark_blue_solid_a"
            android:orientation="horizontal">

            <include layout="@layout/actionbar_settings"></include>

        </RelativeLayout>

            <com.deeptruth.app.android.views.customfonttextview
                style="@style/txt_xapi_detail"
                android:id="@+id/txt_starttime"
                app:fontType="Comfortaa-Regular"
                android:textColor="@color/black"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/grey_x"
                />
            <com.deeptruth.app.android.views.customfonttextview
                style="@style/txt_xapi_detail"
                android:id="@+id/txt_endtime"
                app:fontType="Comfortaa-Regular"
                android:textColor="@color/black"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/grey_x"
                />

            <com.deeptruth.app.android.views.customfonttextview
                style="@style/txt_xapi_detail"
                android:id="@+id/txt_config"
                app:fontType="Comfortaa-Regular"
                android:textColor="@color/black"
                />

            <View
                android:id="@+id/view_started"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:visibility="visible"
                android:background="@color/grey_x"
                />

            <com.deeptruth.app.android.views.customfonttextview
                style="@style/txt_xapi_detail"
                android:id="@+id/txt_key"
                app:fontType="Comfortaa-Regular"
                android:textColor="@color/black"
                />

            <View
                android:id="@+id/view_key"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:visibility="visible"
                android:background="@color/grey_x"
                />

            <com.deeptruth.app.android.views.customfonttextview
                style="@style/txt_xapi_detail"
                android:id="@+id/txt_token"
                app:fontType="Comfortaa-Regular"
                android:textColor="@color/black"
                />

            <View
                android:id="@+id/view_token"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:visibility="visible"
                android:background="@color/grey_x"
                />

            <com.deeptruth.app.android.views.customfonttextview
                style="@style/txt_xapi_detail"
                android:id="@+id/txt_startframe"
                app:fontType="Comfortaa-Regular"
                android:textColor="@color/black"
                android:visibility="visible"
                />

            <View
                android:id="@+id/view_startframe"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:visibility="visible"
                android:background="@color/grey_x"
                />

            <com.deeptruth.app.android.views.customfonttextview
                style="@style/txt_xapi_detail"
                android:id="@+id/txt_endframe"
                app:fontType="Comfortaa-Regular"
                android:textColor="@color/black"
                android:visibility="visible"
                />

            <View
                android:id="@+id/view_endframe"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:visibility="visible"
                android:background="@color/grey_x"
                />

            <com.deeptruth.app.android.views.customfonttextview
                style="@style/txt_xapi_detail"
                android:id="@+id/txt_complete"
                app:fontType="Comfortaa-Regular"
                android:textColor="@color/black"
                android:visibility="visible"
                />

                <View
                    android:id="@+id/view_completeframe"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey_x"
                    />

            <com.deeptruth.app.android.views.customfonttextview
                style="@style/txt_xapi_detail"
                android:id="@+id/txt_totleframe"
                app:fontType="Comfortaa-Regular"
                android:textColor="@color/black"
                android:visibility="visible"
                />

                <View
                    android:id="@+id/view_totleframe"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey_x"
                    />

            <com.deeptruth.app.android.views.customfonttextview
                    style="@style/txt_xapi_detail"
                    android:id="@+id/txt_parameter"
                    app:fontType="Comfortaa-Regular"
                    android:textColor="@color/black"
                    />
                <View
                    android:id="@+id/view_completless"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:visibility="visible"
                    android:background="@color/grey_x"
                    />

            <com.deeptruth.app.android.views.customfonttextview
                style="@style/txt_xapi_detail"
                android:text="@string/result"
                android:paddingLeft="@dimen/margin_10dp"
                android:textSize="17sp"
                android:textStyle="bold"
                app:fontType="Comfortaa-Regular"
                android:gravity="center_horizontal"
                android:textColor="@color/black"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/grey_x"
                />
            <com.deeptruth.app.android.views.customfonttextview
                style="@style/txt_xapi_detail"
                android:id="@+id/txt_result"
                app:fontType="Comfortaa-Regular"
                android:textColor="@color/black"
                android:layout_gravity="center_horizontal"
                />

    </LinearLayout>
</ScrollView>