    <!--publish text details-->
    <string name="publishtext_line1">Publishing your recordings to</string>
    <string name="publishtext_line2">the DeepTruth website allows</string>
    <string name="publishtext_line3">you to share any validated and</string>
    <string name="publishtext_line4">authenticated videos/audios.</string>
    <string name="publishtext_line5">After a recording is uploaded,</string>
    <string name="publishtext_line6">it can be shared on websites</string>
    <string name="publishtext_line7">and social media platforms.</string>

    <!--savetext details-->
    <string name="savetext_line1">The \"SAVE\" option will make a</string>
    <string name="savetext_line2">copy of your recording directly</string>
    <string name="savetext_line3">on to your phone\'s camera roll.</string>
    <string name="savetext_line5">Warning: Sending your file in</string>
    <string name="savetext_line6">an email or a text message as</string>
    <string name="savetext_line7">an attachment will cause your</string>
    <string name="savetext_line8">recordings to be compressed.</string>
    <string name="savetext_line9">Compression will change your</string>
    <string name="savetext_line10">VideoLock file and cause it to</string>
    <string name="savetext_line11">lose its certified original status.</string>
    <string name="savetext_line13">Your VideoLock files can all be</string>
    <string name="savetext_line14">played by other media players.</string>
    <string name="savetext_line15">However, we recommend that</string>
    <string name="savetext_line16">you keep a copy of all certified</string>
    <string name="savetext_line17">original recordings in this app.</string>

    <!--sharetext_details-->

    <string name="sharetext_line1">Sharing your certified recordings</string>
    <string name="sharetext_line2">is a feature of the VideoLock app.</string>
    <string name="sharetext_line3">Your recorded files only exist on</string>
    <string name="sharetext_line4">your phone--until you\'re ready to</string>
    <string name="sharetext_line5">share with others and the world.</string>


    <string name="sharetext_line7">You have the ability to send your</string>
    <string name="sharetext_line8">VideoLock files securely to others</string>
    <string name="sharetext_line9">so they can verify your recording</string>
    <string name="sharetext_line10">with the VideoLock Reader app.</string>


    <string name="sharetext_line12">...AND...</string>


    <string name="sharetext_line14">You can publish your recordings</string>
    <string name="sharetext_line15">on the VideoLock.com website</string>
    <string name="sharetext_line16">and share VideoLock certified</string>
    <string name="sharetext_line17">original content with the world.</string>

    <!--export_details-->
    <string name="exporttext_line1">Many email/text messaging</string>
    <string name="exporttext_line2">systems will compress your</string>
    <string name="exporttext_line3">file attachments and cause</string>
